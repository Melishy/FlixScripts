local function UMHJUJ_fake_script()
	local script = Instance.new('LocalScript', ESP)

	script.Parent.MouseButton1Click:Connect(function()
		game:GetService("StarterGui"):SetCore("SendNotification", { 
			Title = "Выполнено!";
			Text = "Скрипт запущен.";
			Icon = "0"})
		Duration = 3;
		wait(0.1)
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Melishy/MM2Esp/main/MurderMystery2HighlightESP"))(' Watermelon ?')
	end)
end
coroutine.wrap(UMHJUJ_fake_script)()
local function NLIU_fake_script()
	local script = Instance.new('LocalScript', Speed)

	script.Parent.MouseButton1Click:Connect(function()
		game:GetService("StarterGui"):SetCore("SendNotification", { 
			Title = "Выполнено!";
			Text = "Скрипт запущен.";
			Icon = "0"})
		Duration = 3;
		while true do
			local speed = 21
			local player = game.Players.LocalPlayer
			local character = player.Character
			character.Humanoid.WalkSpeed = speed
		end
	end)
end
coroutine.wrap(NLIU_fake_script)()
local function WTLNHJ_fake_script()
	local script = Instance.new('LocalScript', Fly)

	script.Parent.MouseButton1Click:Connect(function()
		game:GetService("StarterGui"):SetCore("SendNotification", { 
			Title = "Выполнено!";
			Text = "Скрипт запущен.";
			Icon = "0"})
		Duration = 3;
		wait(0.1)
		loadstring(game:HttpGet(('https://raw.githubusercontent.com/Melishy/ScriptsByMelishy/main/FlyByMelishy.lua'),true))()
	end)
end
coroutine.wrap(WTLNHJ_fake_script)()
local function YHPHGYU_fake_script()
	local script = Instance.new('LocalScript', FarmBalls)

	script.Parent.MouseButton1Click:Connect(function()
